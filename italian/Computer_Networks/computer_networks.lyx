\begin_layout Section*
Protocollo 
\emph on
ALOHA
\end_layout

\begin_layout Itemize
Niente carrier sensing, niente collision detection
\end_layout

\begin_layout Itemize
\begin_inset Formula $T_{T}=\text{Tempo di trasmissione del pacchetto}$
\end_inset


\end_layout

\begin_layout Itemize
\begin_inset Formula $S=\text{Numero medio di trasmissioni con successo in un intervallo di tempo }T$
\end_inset


\end_layout

\begin_layout Itemize
\begin_inset Formula $G=\text{Numero medio di trasmissioni tentate in un intervallo di tempo }T$
\end_inset

 
\end_layout

\begin_layout Itemize
Arrivo dei pacchetti con distribuzione di 
\emph on
Poisson
\emph default
 con 
\begin_inset Formula $\Lambda=\frac{G}{T}$
\end_inset


\end_layout

\begin_layout Itemize
\begin_inset Formula $S=\left\{ \text{Probabilità di passare}\right\} =G\cdot\left\{ \text{Probabilità che in }2\cdot T\text{ non trasmette nessuno}\right\} =G\cdot e^{-2\cdot G}$
\end_inset


\end_layout

\begin_deeper
\begin_layout Itemize
Il massimo si ottiene per 
\begin_inset Formula $G=\frac{1}{2}\;\Rightarrow\; S=0,184$
\end_inset

 cioè passa 
\series bold
al massimo solo il 
\begin_inset Formula $18\%$
\end_inset

 del traffico!
\end_layout

\end_deeper
\begin_layout Itemize

\series bold
In base alla metrica del ritardo:
\end_layout

\begin_deeper
\begin_layout Itemize
\begin_inset Formula $D_{ALOHA}=\left(e^{2\cdot G}-1\right)\cdot\left(T_{T}+2\cdot\tau+\frac{k-1}{2}\cdot T_{T}\right)+T_{T}+\tau$
\end_inset


\end_layout

\begin_deeper
\begin_layout Itemize
Il termine 
\begin_inset Formula $\left(e^{2\cdot G}-1\right)$
\end_inset

 rappresenta il numero di tentativi senza successo per ottenere una trasmissione
 che vada a buon fine.
\end_layout

\begin_layout Itemize
Il termine 
\begin_inset Formula $\frac{k-1}{2}\cdot T_{T}$
\end_inset

 rappresenta la media del tempo di 
\emph on
backoff
\emph default
.
\end_layout

\end_deeper
\begin_layout Itemize
Abbiamo quindi un 
\series bold
delay di attraversamento medio normalizzato
\series default
 pari a 
\begin_inset Formula $\hat{D}_{ALOHA}=\left(e^{2\cdot G}-1\right)\cdot\left(\frac{k+1}{2}+2\cdot a\right)+1+a$
\end_inset

 
\end_layout

\begin_layout Itemize
Se aumento 
\begin_inset Formula $S$
\end_inset

 e 
\begin_inset Formula $k$
\end_inset

 , 
\begin_inset Formula $D$
\end_inset

 tende ad esplodere
\end_layout

\end_deeper
