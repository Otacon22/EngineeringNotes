\begin_layout Itemize

\series bold
Definiamo un polinomio
\series default
 
\begin_inset Formula $P\left(x\right)$
\end_inset

 di grado 
\begin_inset Formula $n-1$
\end_inset


\begin_inset Formula 
\[
P\left(x\right)=b_{n}\cdot X^{n-1}+b_{n-1}\cdot X^{n-2}+...+b_{1}\cdot X+b_{0}\cdot1
\]

\end_inset


\end_layout

\begin_layout Itemize

\series bold
Polinomio divisore
\series default
 
\begin_inset Formula $D\left(x\right)$
